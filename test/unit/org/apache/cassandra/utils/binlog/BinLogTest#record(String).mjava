    static BinLog.ReleaseableWriteMarshallable record(String text)
    {
        return new BinLog.ReleaseableWriteMarshallable()
        {
            protected void release()
            {
                //Do nothing
            }

            public void writeMarshallable(WireOut wire)
            {
                wire.write("text").text(text);
            }
        };
    }

