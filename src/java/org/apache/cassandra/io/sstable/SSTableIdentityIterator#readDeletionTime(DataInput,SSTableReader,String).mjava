    private static DeletionTime readDeletionTime(DataInput in, SSTableReader sstable, String filename)
    {
        try
        {
            return DeletionTime.serializer.deserialize(in);
        }
        catch (IOException e)
        {
            if (sstable != null)
                sstable.markSuspect();
            throw new CorruptSSTableException(e, filename);
        }
    }

