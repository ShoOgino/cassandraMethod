        public HashCode hash()
        {
            byte[] res = new byte[bits / 8];
            int i = 0;
            for (Hasher hasher : underlying)
            {
                HashCode newHash = hasher.hash();
                i += newHash.writeBytesTo(res, i, newHash.bits() / 8);
            }
            return HashCode.fromBytes(res);
        }

