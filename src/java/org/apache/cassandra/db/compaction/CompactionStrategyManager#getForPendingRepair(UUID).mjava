    @VisibleForTesting
    List<AbstractCompactionStrategy> getForPendingRepair(UUID sessionID)
    {
        readLock.lock();
        try
        {
            List<AbstractCompactionStrategy> strategies = new ArrayList<>(pendingRepairs.size());
            pendingRepairs.forEach(p -> strategies.add(p.get(sessionID)));
            return strategies;
        }
        finally
        {
            readLock.unlock();
        }
    }

