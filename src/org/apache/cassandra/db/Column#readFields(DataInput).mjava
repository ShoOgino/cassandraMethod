    /**
     * This method is basically implemented for Writable interface
     * for M/R. 
     */
    public void readFields(DataInput in) throws IOException
    {
        name_ = in.readUTF();
        boolean delete = in.readBoolean();
        long ts = in.readLong();
        int size = in.readInt();
        byte[] value = new byte[size];
        in.readFully(value);        
        if ( delete )
            delete();
    }

