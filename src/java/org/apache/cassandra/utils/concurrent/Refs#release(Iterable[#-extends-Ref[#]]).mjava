    public static void release(Iterable<? extends Ref<?>> refs)
    {
        Throwable fail = null;
        for (Ref ref : refs)
        {
            try
            {
                ref.release();
            }
            catch (Throwable t)
            {
                fail = merge(fail, t);
            }
        }
        if (fail != null)
            throw Throwables.propagate(fail);
    }

