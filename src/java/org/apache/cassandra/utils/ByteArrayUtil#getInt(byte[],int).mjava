    /**
     * @return signed int from the given offset encoded as big endian
     */
    public static int getInt(byte[] b, int off)
    {
        return (b[off    ] & 255) << 24 |
               (b[off + 1] & 255) << 16 |
               (b[off + 2] & 255) <<  8 |
               (b[off + 3] & 255);
    }

