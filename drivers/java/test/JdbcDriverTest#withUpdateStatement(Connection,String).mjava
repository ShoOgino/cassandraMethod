    /**
     * With update statement
     * 
     * @param con
     *            connection object.
     * @param updateQ
     *            update query.
     * @throws SQLException
     *             sql exception.
     */
    private static void withUpdateStatement(final Connection con, final String updateQ) throws SQLException
    {
        PreparedStatement statement = con.prepareStatement(updateQ);
        statement.execute();
    }

