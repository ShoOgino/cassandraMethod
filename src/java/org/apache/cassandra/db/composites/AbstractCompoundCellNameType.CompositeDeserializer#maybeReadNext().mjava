        private void maybeReadNext() throws IOException
        {
            if (nextFull != null)
                return;

            nextIdx = 0;
            nextSize = 0;

            int length = in.readShort() & 0xFFFF;
            // Note that empty is ok because it marks the end of row
            if (length == 0)
            {
                nextFull = EMPTY;
                return;
            }

            nextFull = new byte[length];
            in.readFully(nextFull);
        }

