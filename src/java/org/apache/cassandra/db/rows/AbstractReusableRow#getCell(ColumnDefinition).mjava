    public Cell getCell(ColumnDefinition c)
    {
        assert !c.isComplex();
        if (data().simpleData == null)
            return null;

        int idx = columns().simpleIdx(c, 0);
        if (idx < 0)
            return null;

        return simpleCell().setTo(data().simpleData.data, c, (row() * columns().simpleColumnCount()) + idx);
    }

