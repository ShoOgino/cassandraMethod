    public V get(K k)
    {
        if (cache == null)
            return loadFunction.apply(k);

        try {
            return cache.get(k);
        }
        catch (ExecutionException | UncheckedExecutionException e)
        {
            Throwables.propagateIfInstanceOf(e.getCause(), RuntimeException.class);
            throw Throwables.propagate(e);
        }
    }

