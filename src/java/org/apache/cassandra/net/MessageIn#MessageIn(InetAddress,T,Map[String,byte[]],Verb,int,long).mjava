    private MessageIn(InetAddress from,
                      T payload,
                      Map<String, byte[]> parameters,
                      Verb verb,
                      int version,
                      long constructionTime)
    {
        this.from = from;
        this.payload = payload;
        this.parameters = parameters;
        this.verb = verb;
        this.version = version;
        this.constructionTime = constructionTime;
    }

