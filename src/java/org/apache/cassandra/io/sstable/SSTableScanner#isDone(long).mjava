    private boolean isDone(long current)
    {
        if (current < stopAt)
            return false;

        if (!hasSeeked)
        {
            // We're wrapping, so seek to the start now (which sets hasSeeked)
            seekToStart();
            stopAt = dfile.length();
        }
        // Re-test now that we might have seeked
        return current >= stopAt;
    }

