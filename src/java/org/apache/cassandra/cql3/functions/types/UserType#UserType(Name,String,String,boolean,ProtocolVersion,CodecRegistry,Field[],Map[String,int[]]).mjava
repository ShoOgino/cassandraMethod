    private UserType(
    Name name,
    String keyspace,
    String typeName,
    boolean frozen,
    ProtocolVersion protocolVersion,
    CodecRegistry codecRegistry,
    Field[] byIdx,
    Map<String, int[]> byName)
    {
        super(name);
        this.keyspace = keyspace;
        this.typeName = typeName;
        this.frozen = frozen;
        this.protocolVersion = protocolVersion;
        this.codecRegistry = codecRegistry;
        this.byIdx = byIdx;
        this.byName = byName;
    }

