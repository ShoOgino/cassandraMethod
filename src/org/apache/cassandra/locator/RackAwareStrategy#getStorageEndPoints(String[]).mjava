    public Map<String, EndPoint[]> getStorageEndPoints(String[] keys)
    {
    	Map<String, EndPoint[]> results = new HashMap<String, EndPoint[]>();

        for ( String key : keys )
        {
        	BigInteger token = StorageService.hash(key);
            results.put(key, getStorageEndPoints(token));
        }
        
        return results;
    }

