    /**
     * Factory method that gets an instance of the StorageService
     * class.
     */
    public static AnalyticsContext instance()
    {
        if ( instance_ == null )
        {
        	AnalyticsContext.createLock_.lock();
            try
            {
                if ( instance_ == null )
                {
                    instance_ = new AnalyticsContext();
                }
            }
            finally
            {
                createLock_.unlock();
            }
        }
        return instance_;
    }

