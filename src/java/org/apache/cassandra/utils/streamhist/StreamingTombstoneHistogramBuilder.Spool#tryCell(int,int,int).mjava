        private boolean tryCell(int cell, int point, int delta)
        {
            cell = cell % map.length;
            if (map[cell] == -1)
            {
                map[cell] = point;
                map[cell + 1] = delta;
                size++;
                return true;
            }
            if (map[cell] == point)
            {
                map[cell + 1] += delta;
                return true;
            }
            return false;
        }

