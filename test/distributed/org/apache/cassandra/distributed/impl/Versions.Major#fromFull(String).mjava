        static Major fromFull(String version)
        {
            if (version.isEmpty())
                throw new IllegalArgumentException(version);
            switch (version.charAt(0))
            {
                case '2':
                    if (version.startsWith("2.2"))
                        return v22;
                    throw new IllegalArgumentException(version);
                case '3':
                    if (version.startsWith("3.0"))
                        return v30;
                    return v3X;
                case '4':
                    return v4;
                default:
                    throw new IllegalArgumentException(version);
            }
        }

