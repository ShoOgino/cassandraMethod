    /*
     * Factory method that gets an instance of the StorageService
     * class.
    */
    public static LeaderElector instance()
    {
        if ( instance_ == null )
        {
            LeaderElector.createLock_.lock();
            try
            {
                if ( instance_ == null )
                {
                    instance_ = new LeaderElector();
                }
            }
            finally
            {
                createLock_.unlock();
            }
        }
        return instance_;
    }

