    /*
     * Returns a safe clone of endPointTokenMap_.
    */
    public Map<InetAddress, Token> cloneEndPointTokenMap()
    {
        lock.readLock().lock();
        try
        {            
            return new HashMap<InetAddress, Token>(tokenToEndPointMap.inverse());
        }
        finally
        {
            lock.readLock().unlock();
        }
    }

