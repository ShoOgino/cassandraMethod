    static Row resolveSuperset(List<Row> rows)
    {
        assert rows.size() > 0;
        Row resolved = null;
        for (Row row : rows)
        {
            if (row.cf != null)
            {
                resolved = new Row(row.key, row.cf.cloneMe());
                break;
            }
        }
        if (resolved == null)
            return rows.get(0);
        for (Row row : rows)
        {
            resolved = resolved.resolve(row);
        }
        return resolved;
    }

