    /** takes nanoseconds **/
    public void addNano(long nanos)
    {
        // convert to microseconds. 1 millionth
        latency.update(nanos, TimeUnit.NANOSECONDS);
        totalLatency.inc(nanos);
        for(LatencyMetrics parent : parents)
        {
            parent.addNano(nanos);
        }
    }

