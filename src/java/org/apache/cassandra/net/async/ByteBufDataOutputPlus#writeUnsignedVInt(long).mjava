    @Override
    public void writeUnsignedVInt(long v) throws IOException
    {
        int size = VIntCoding.computeUnsignedVIntSize(v);
        if (size == 1)
        {
            buffer.writeByte((byte) (v & 0xFF));
            return;
        }

        buffer.writeBytes(VIntCoding.encodeVInt(v, size), 0, size);
    }

