    @VisibleForTesting
    Set<UUID> pendingRepairs()
    {
        readLock.lock();
        try
        {
            Set<UUID> ids = new HashSet<>();
            pendingRepairs.getManagers().forEach(p -> ids.addAll(p.getSessions()));
            return ids;
        }
        finally
        {
            readLock.unlock();
        }
    }

