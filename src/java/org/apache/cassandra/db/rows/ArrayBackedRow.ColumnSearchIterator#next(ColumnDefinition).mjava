        public ColumnData next(ColumnDefinition column)
        {
            int idx = binarySearch(column, searchFrom, size);
            if (idx < 0)
            {
                searchFrom = -idx - 1;
                return null;
            }
            else
            {
                // We've found it. We'll start after it next time.
                searchFrom = idx + 1;
                return data[idx];
            }
        }

