  public void setFieldValue(int fieldID, Object value) {
    switch (fieldID) {
    case COLUMNNAME:
      setColumnName((String)value);
      break;

    case VALUE:
      setValue((byte[])value);
      break;

    case TIMESTAMP:
      setTimestamp((Long)value);
      break;

    default:
      throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
    }
  }

