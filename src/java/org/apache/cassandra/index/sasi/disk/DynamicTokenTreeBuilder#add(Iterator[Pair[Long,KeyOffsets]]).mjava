    public void add(Iterator<Pair<Long, KeyOffsets>> data)
    {
        while (data.hasNext())
        {
            Pair<Long, KeyOffsets> entry = data.next();
            for (LongObjectCursor<long[]> cursor : entry.right)
                for (long l : cursor.value)
                    add(entry.left, cursor.key, l);
        }
    }

