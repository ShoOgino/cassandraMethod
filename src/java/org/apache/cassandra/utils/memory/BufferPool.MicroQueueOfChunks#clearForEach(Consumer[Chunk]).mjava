        private void clearForEach(Consumer<Chunk> consumer)
        {
            Chunk chunk0 = this.chunk0, chunk1 = this.chunk1, chunk2 = this.chunk2;
            this.chunk0 = this.chunk1 = this.chunk2 = null;
            forEach(consumer, count, chunk0, chunk1, chunk2);
            count = 0;
        }

