    /*
     * This is called after bootstrap to add the files
     * to the list of files maintained.
    */
    void addToList(SSTableReader file)
    {
        lock_.writeLock().lock();
        try
        {
            ssTables_.put(file.getFilename(), file);
        }
        finally
        {
            lock_.writeLock().unlock();
        }
    }

