    private static byte[] array(int start, int size)
    {
        assert Byte.MIN_VALUE <= start && start <= Byte.MAX_VALUE;
        assert (start + size) <= Byte.MAX_VALUE;
        byte[] a = new byte[size];
        for (int i=0; i<size; i++)
            a[i] = (byte) (start + i);
        return a;
    }

