        void remove(int prev, int next)
        {
            long key = getKey(prev, next);
            int index = Arrays.binarySearch(data, key);
            if (index >= 0)
            {
                if (index < data.length)
                    System.arraycopy(data, index + 1, data, index, data.length - index - 1);
                data[data.length - 1] = EMPTY;
            }
        }

