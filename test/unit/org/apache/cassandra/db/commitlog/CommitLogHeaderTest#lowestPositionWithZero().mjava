    @Test
    public void lowestPositionWithZero()
    {
        CommitLogHeader clh = new CommitLogHeader();
        clh.turnOn(2, 34);
        assert clh.getReplayPosition() == 34;
        clh.turnOn(100, 0);
        assert clh.getReplayPosition() == 0;
        clh.turnOn(65, 2);
        assert clh.getReplayPosition() == 0;
    }

