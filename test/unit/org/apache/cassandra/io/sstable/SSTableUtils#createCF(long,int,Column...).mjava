    public static ColumnFamily createCF(long mfda, int ldt, Column... cols)
    {
        ColumnFamily cf = TreeMapBackedSortedColumns.factory.create(KEYSPACENAME, CFNAME);
        cf.delete(new DeletionInfo(mfda, ldt));
        for (Column col : cols)
            cf.addColumn(col);
        return cf;
    }

