    /*
     * Returns a safe clone of tokenToEndPointMap_.
    */
    public Map<Token, InetAddress> cloneTokenEndPointMap()
    {
        lock_.readLock().lock();
        try
        {            
            return new HashMap<Token, InetAddress>( tokenToEndPointMap_ );
        }
        finally
        {
            lock_.readLock().unlock();
        }
    }

