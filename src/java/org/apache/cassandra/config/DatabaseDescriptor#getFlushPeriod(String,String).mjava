    public static int getFlushPeriod(String tableName, String columnFamilyName)
    {
        assert tableName != null;
        CFMetaData cfMetaData = getCFMetaData(tableName, columnFamilyName);
        
        if (cfMetaData == null)
            return 0;
        return cfMetaData.flushPeriodInMinutes;
    }

