    // For debugging, not fast
    String dirtyString()
    {
        StringBuilder sb = new StringBuilder();
        for (Integer cfId : cfDirty)
        {
            CFMetaData m = DatabaseDescriptor.getCFMetaData(cfId);
            sb.append(m == null ? m.cfName : "<deleted>").append(" (").append(cfId).append("), ");
        }
        return sb.toString();
    }

