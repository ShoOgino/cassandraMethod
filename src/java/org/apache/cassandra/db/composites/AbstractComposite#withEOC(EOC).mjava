    public Composite withEOC(EOC newEoc)
    {
        // Note: CompositeBound overwrite this so we assume the EOC of this is NONE
        switch (newEoc)
        {
            case START:
                return BoundedComposite.startOf(this);
            case END:
                return BoundedComposite.endOf(this);
            default:
                return this;
        }
    }

