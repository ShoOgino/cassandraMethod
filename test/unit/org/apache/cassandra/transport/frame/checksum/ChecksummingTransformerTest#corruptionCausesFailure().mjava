    @Test
    public void corruptionCausesFailure()
    {
        qt().withExamples(500)
            .forAll(inputWithCorruptablePosition(),
                    integers().between(0, Byte.MAX_VALUE).map(Integer::byteValue),
                    compressors(),
                    checksumTypes())
            .checkAssert(this::roundTripWithCorruption);
    }

