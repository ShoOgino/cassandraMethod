    @Test
    public void testParsing()
    {
        SemanticVersion version;

        version = new SemanticVersion("1.2.3");
        assert version.major == 1 && version.minor == 2 && version.patch == 3;

        version = new SemanticVersion("1.2.3-foo.2+Bar");
        assert version.major == 1 && version.minor == 2 && version.patch == 3;
    }

