    public static TypeInfo fromByte(byte b)
    {
        TypeInfo ti = null;
        switch(b)
        {
            case 1:
                ti = TypeInfo.BYTE;                
                break;
                
            case 2:
                ti = TypeInfo.CHAR;
                break;
                
            case 3:
                ti = TypeInfo.SHORT;
                break;
                
            case 4:
                ti = TypeInfo.INT;
                break;
                
            case 5:
                ti = TypeInfo.LONG;
                break;
                
            case 6:
                ti = TypeInfo.DOUBLE;
                break;
                
            case 7:
                ti = TypeInfo.FLOAT;
                break;
                
            case 8:
                ti = TypeInfo.STRING;
                break;
                
            case 9:
                ti = TypeInfo.BLOB;
                break;               
        }
        return ti;
    }

