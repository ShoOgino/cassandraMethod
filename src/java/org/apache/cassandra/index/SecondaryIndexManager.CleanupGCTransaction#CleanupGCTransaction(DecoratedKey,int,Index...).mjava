        private CleanupGCTransaction(DecoratedKey key,
                                     int nowInSec,
                                     Index...indexes)
        {
            // don't allow null indexers, if we don't have any, use a noop transaction
            for (Index index : indexes) assert index != null;

            this.key = key;
            this.indexes = indexes;
            this.nowInSec = nowInSec;
        }

