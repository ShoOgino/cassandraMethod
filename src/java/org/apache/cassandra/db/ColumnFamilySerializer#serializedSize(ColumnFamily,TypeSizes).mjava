    public long serializedSize(ColumnFamily cf, TypeSizes type)
    {
        if (cf == null)
        {
            return type.sizeof(false);
        }
        else
        {
            return type.sizeof(true) +      /* nullness bool */
                   type.sizeof(cf.id()) +   /* id */
                   serializedSizeForSSTable(cf, type);
        }
    }

