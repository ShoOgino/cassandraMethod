    @Override
    public boolean equals(Object o)
    {
        if (this == o)
            return true;

        if (!(o instanceof StreamingHistogram))
            return false;

        StreamingHistogram that = (StreamingHistogram) o;
        return maxBinSize == that.maxBinSize &&
               maxSpoolSize == that.maxSpoolSize &&
               spool.equals(that.spool) &&
               bin.equals(that.bin);
    }

