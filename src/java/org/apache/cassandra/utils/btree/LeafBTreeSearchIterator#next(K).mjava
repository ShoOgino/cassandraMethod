    public V next(K key)
    {
        if (!hasNext)
            return null;
        V result = null;

        int find = searchNext(key);
        if (find >= 0)
        {
            hasCurrent = true;
            result = (V) keys[find];
            nextPos = find + (forwards ? 1 : -1);
        }
        else
        {
            nextPos = (forwards ? -1 : -2) - find;
            hasCurrent = false;
        }
        hasNext = nextPos >= lowerBound && nextPos <= upperBound;
        return result;
    }

