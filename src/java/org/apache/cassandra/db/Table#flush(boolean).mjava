    public void flush(boolean fRecovery) throws IOException
    {
        for (String cfName : columnFamilyStores_.keySet())
        {
            if (fRecovery)
            {
                columnFamilyStores_.get(cfName).flushMemtableOnRecovery();
            }
            else
            {
                columnFamilyStores_.get(cfName).forceFlush();
            }
        }
    }

