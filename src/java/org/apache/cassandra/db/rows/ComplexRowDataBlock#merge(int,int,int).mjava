    // Merge row i into j
    public void merge(int i, int j, int nowInSec)
    {
        assert i > j;

        mergeCells(i, j, nowInSec);

        int s = columns.complexColumnCount();
        if (i * s >= complexDelTimes.size())
            return;

        for (int k = 0; k < s; k++)
            if (complexDelTimes.supersedes(i * s + k, j * s + k))
                complexDelTimes.move(i * s + k, j * s + k);
    }

