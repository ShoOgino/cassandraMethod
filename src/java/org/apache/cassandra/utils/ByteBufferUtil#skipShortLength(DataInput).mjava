    /**
     * @param in data input
     * @return null
     * @throws IOException if an I/O error occurs.
     */
    public static void skipShortLength(DataInput in) throws IOException
    {
        int skip = readShortLength(in);
        while (skip > 0)
        {
            int skipped = in.skipBytes(skip);
            if (skipped == 0) throw new EOFException();
            skip -= skipped;
        }
    }

