    @Test
    public void testSSTableExport_NoArgs()
    {
        runTool(1, "org.apache.cassandra.tools.SSTableExport");
        assertNoUnexpectedThreadsStarted(null, OPTIONAL_THREADS_WITH_SCHEMA);
