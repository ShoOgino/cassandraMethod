    protected void expectEOF(Callable<?> callable)
    {
        boolean threw = false;
        try
        {
            callable.call();
        }
        catch (Exception e)
        {
            assert e.getClass().equals(EOFException.class) : e.getClass().getName() + " is not " + EOFException.class.getName();
            threw = true;
        }
        assert threw : EOFException.class.getName() + " not received";
    }

