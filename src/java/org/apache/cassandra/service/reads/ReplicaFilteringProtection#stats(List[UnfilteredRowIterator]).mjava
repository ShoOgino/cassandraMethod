    private static EncodingStats stats(List<UnfilteredRowIterator> iterators)
    {
        EncodingStats stats = EncodingStats.NO_STATS;
        for (UnfilteredRowIterator iter : iterators)
        {
            if (iter == null)
                continue;

            stats = stats.mergeWith(iter.stats());
        }
        return stats;
    }

