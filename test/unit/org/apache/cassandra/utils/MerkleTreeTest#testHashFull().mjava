    @Test
    public void testHashFull()
    {
        byte[] val = DUMMY;
        Range range = new Range(tok(0), tok(0));

        // (zero, zero]
        assertNull(mt.hash(range));
        
        // validate the range
        mt.get(tok(0)).hash(val);
        
        assertHashEquals(val, mt.hash(range));
    }

