    public int compare(ByteBuffer o1, ByteBuffer o2)
    {
        if ((o1 == null) || (o1.remaining() != 1))
            return ((o2 == null) || (o2.remaining() != 1)) ? 0 : -1;
        if ((o2 == null) || (o2.remaining() != 1))
            return 1;

        // False is 0, True is anything else, makes False sort before True.
        byte b1 = o1.get();
        byte b2 = o2.get();
        if (b1 == 0)
            return b2 == 0 ? 0 : -1;
        return b2 == 0 ? 1 : 0;
    }

