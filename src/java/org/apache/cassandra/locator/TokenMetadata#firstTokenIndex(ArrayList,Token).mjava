    public static int firstTokenIndex(final ArrayList ring, Token start)
    {
        assert ring.size() > 0;
        int i = Collections.binarySearch(ring, start);
        if (i < 0)
        {
            i = (i + 1) * (-1);
            if (i >= ring.size())
            {
                i = 0;
            }
        }
        return i;
    }

