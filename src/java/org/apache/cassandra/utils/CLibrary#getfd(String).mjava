    public static int getfd(String path)
    {
        try(FileChannel channel = FileChannel.open(Paths.get(path), StandardOpenOption.READ))
        {
            return getfd(channel);
        }
        catch (IOException e)
        {
            JVMStabilityInspector.inspectThrowable(e);
            // ignore
            return -1;
        }
    }

