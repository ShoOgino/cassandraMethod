    @Test
    public void testIsNameValidNegative()
    {
        assertFalse(SchemaConstants.isValidName(null));
        assertFalse(SchemaConstants.isValidName(""));
        assertFalse(SchemaConstants.isValidName(" "));
        assertFalse(SchemaConstants.isValidName("@"));
        assertFalse(SchemaConstants.isValidName("!"));
    }

