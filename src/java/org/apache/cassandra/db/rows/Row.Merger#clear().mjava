        public void clear()
        {
            Arrays.fill(rows, null);
            Arrays.fill(cells, null);
            if (complexDelTimes != null)
                Arrays.fill(complexDelTimes, null);
            complexCells.clear();
            rowsToMerge = 0;

            rowInfo = LivenessInfo.NONE;
            rowDeletion = DeletionTime.LIVE;

            signaledListenerForRow = false;
        }

