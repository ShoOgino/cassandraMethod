    /**
     * With Delete statement
     * 
     * @param con
     *            connection object.
     * @param deleteQ
     *            delete query.
     * @throws SQLException
     *             sql exception.
     */
    private static void withDeleteStatement(final Connection con, final String deleteQ) throws SQLException
    {
        PreparedStatement statement = con.prepareStatement(deleteQ);
        statement.execute();
    }
