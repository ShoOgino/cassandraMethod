    /*
     * Returns a safe clone of endPointTokenMap_.
    */
    public Map<EndPoint, BigInteger> cloneEndPointTokenMap()
    {
        lock_.readLock().lock();
        try
        {            
            return new HashMap<EndPoint, BigInteger>( endPointToTokenMap_ );
        }
        finally
        {
            lock_.readLock().unlock();
        }
    }

