    public insert_args(
      String table,
      String key,
      ColumnPath column_path,
      byte[] value,
      long timestamp,
      int block_for)
    {
      this();
      this.table = table;
      this.key = key;
      this.column_path = column_path;
      this.value = value;
      this.timestamp = timestamp;
      this.__isset.timestamp = true;
      this.block_for = block_for;
      this.__isset.block_for = true;
    }

