    /*
     * Returns a safe clone of endPointTokenMap_.
    */
    public Map<InetAddress, Token> cloneEndPointTokenMap()
    {
        lock_.readLock().lock();
        try
        {            
            return new HashMap<InetAddress, Token>( endPointToTokenMap_ );
        }
        finally
        {
            lock_.readLock().unlock();
        }
    }

