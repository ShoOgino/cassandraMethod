    /**
     * Process received file.
     *
     * @param sstable SSTable file received.
     */
    public void received(SSTableWriter sstable)
    {
        assert cfId.equals(sstable.metadata.cfId);
        assert !aborted;

        sstables.add(sstable);
        if (sstables.size() == totalFiles)
            complete();
    }

