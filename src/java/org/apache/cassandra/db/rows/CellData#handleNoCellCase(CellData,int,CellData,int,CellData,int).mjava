    private static boolean handleNoCellCase(CellData d1, int i1, CellData d2, int i2, CellData merged, int iMerged)
    {
        if (!d1.hasCell(i1))
        {
            if (d2.hasCell(i2))
                d2.moveCell(i2, merged, iMerged);
            return true;
        }
        if (!d2.hasCell(i2))
        {
            d1.moveCell(i1, merged, iMerged);
            return true;
        }
        return false;
    }

