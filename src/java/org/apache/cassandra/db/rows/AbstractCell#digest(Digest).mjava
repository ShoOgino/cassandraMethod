    public void digest(Digest digest)
    {
        if (isCounterCell())
            digest.updateWithCounterContext(value());
        else
            digest.update(value());

        digest.updateWithLong(timestamp())
              .updateWithInt(ttl())
              .updateWithBoolean(isCounterCell());
        if (path() != null)
            path().digest(digest);
    }

