    public Cell getCell(ColumnDefinition c)
    {
        assert !c.isComplex();
        if (data.simpleData == null)
            return null;

        int idx = columns().simpleIdx(c, 0);
        if (idx < 0)
            return null;

        return SimpleRowDataBlock.reusableCell().setTo(data.simpleData.data, c, idx);
    }

