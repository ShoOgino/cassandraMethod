    private static Filter createFilter(int hash, long numElements, int bucketsPer, Type type)
    {
        switch (type)
        {
            case MURMUR2:
              return new Murmur2BloomFilter(hash, numElements, bucketsPer);
            default:
              return new Murmur3BloomFilter(hash, numElements, bucketsPer);
        }
    }

