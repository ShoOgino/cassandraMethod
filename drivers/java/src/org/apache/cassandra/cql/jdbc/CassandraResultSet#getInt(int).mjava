    /**
     * @param index
     * @return
     * @throws SQLException
     */
    public int getInt(int index) throws SQLException
    {
        TypedColumn column = values.get(index - 1);
        assert column != null;
        Object value = column.getValue();
        wasNull = value == null;
        return value == null ? 0 : ((BigInteger) value).intValue();
    }

