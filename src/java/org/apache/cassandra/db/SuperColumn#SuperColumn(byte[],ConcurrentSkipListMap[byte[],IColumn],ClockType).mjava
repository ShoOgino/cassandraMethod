    private SuperColumn(byte[] name, ConcurrentSkipListMap<byte[], IColumn> columns, ClockType clockType)
    {
        assert name != null;
        assert name.length <= IColumn.MAX_NAME_LENGTH;
    	name_ = name;
        columns_ = columns;
        markedForDeleteAt = new AtomicReference<IClock>(clockType.minClock());
    }

