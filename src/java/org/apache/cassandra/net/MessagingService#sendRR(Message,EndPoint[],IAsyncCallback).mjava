    public String sendRR(Message message, EndPoint[] to, IAsyncCallback cb)
    {
        String messageId = message.getMessageId();                        
        callbackMap_.put(messageId, cb);
        for ( int i = 0; i < to.length; ++i )
        {
            sendOneWay(message, to[i]);
        }
        return messageId;
    }

