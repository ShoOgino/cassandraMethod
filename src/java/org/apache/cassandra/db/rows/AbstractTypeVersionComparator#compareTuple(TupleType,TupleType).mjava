    private int compareTuple(TupleType type, TupleType otherType)
    {
        if (type.size() != otherType.size())
            return Integer.compare(type.size(), otherType.size());

        int test = 0;
        int i = 0;
        while (test == 0 && i < type.size())
        {
            test = compare(type.type(i), otherType.type(i));
            i++;
        }
        return test;
    }

