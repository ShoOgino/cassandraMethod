    private static byte[] getNextMinKey(byte[] cur, byte[][] keys)
    {
        // find max
        for (byte[] key : keys)
            if (FBUtilities.compareUnsigned(cur, key) < 0)
                cur = key;

        // increment
        for (int i = 0 ; i < cur.length ; i++)
            if (++cur[i] != 0)
                break;
        return cur;
    }

