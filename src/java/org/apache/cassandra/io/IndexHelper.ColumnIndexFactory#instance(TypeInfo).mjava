        public static ColumnIndexInfo instance(TypeInfo typeInfo)
        {
            ColumnIndexInfo cIndexInfo = null;
            switch(typeInfo)
            {
                case STRING:
                    cIndexInfo = new ColumnNameIndexInfo();
                    break;
                    
                case LONG:
                    cIndexInfo = new ColumnTimestampIndexInfo();
                    break;
            }
            return cIndexInfo;
        }    

