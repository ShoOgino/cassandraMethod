	@Override	public void serialize(ReadCommand rm, DataOutputStream dos) throws IOException	{		SliceByRangeReadCommand realRM = (SliceByRangeReadCommand)rm;		dos.writeBoolean(realRM.isDigestQuery());		dos.writeUTF(realRM.table);		dos.writeUTF(realRM.key);		dos.writeUTF(realRM.columnFamily);		dos.writeUTF(realRM.startColumn);		dos.writeUTF(realRM.endColumn);		dos.writeInt(realRM.count);	}
