  /**
   * Find a the enum type by its integer value, as defined in the Thrift IDL.
   * @return null if the value is not found.
   */
  public static IndexType findByValue(int value) { 
    switch (value) {
      case 0:
        return KEYS;
      case 1:
        return CUSTOM;
      default:
        return null;
    }
  }

