    public static boolean delete(List<String> files) throws IOException
    {
        boolean bVal = true;
        for ( int i = 0; i < files.size(); ++i )
        {
            String file = files.get(i);
            bVal = delete(file);
            if (bVal)
            {
            	logger_.debug("Deleted file " + file);
                files.remove(i);
            }
        }
        return bVal;
    }

