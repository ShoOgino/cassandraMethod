    /*
     * Returns a safe clone of tokenToEndPointMap_.
    */
    public Map<Token, InetAddress> cloneTokenEndPointMap()
    {
        lock.readLock().lock();
        try
        {            
            return new HashMap<Token, InetAddress>(tokenToEndPointMap);
        }
        finally
        {
            lock.readLock().unlock();
        }
    }

