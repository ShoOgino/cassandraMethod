    double p(double t)
    {
        // Stat stat = new Stat();
        double mean = mean();        
        double deviation = deviation();   
        /* Exponential CDF = 1 -e^-lambda*x */
        double exponent = (-1)*(t)/mean;
        return 1 - ( 1 - Math.pow(Math.E, exponent) );
        // return stat.gaussianCDF(mean, deviation, t, Double.POSITIVE_INFINITY);             
    }

