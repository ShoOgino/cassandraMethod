    public SequentialScanner(String table) throws IOException
    {
        table_ = table;    
        List<String> allFiles = Table.open(table_).getAllSSTablesOnDisk();
        
        for (String file : allFiles)
        {                                         
            FileStruct fs = new FileStruct(file, SequentialScanner.bufSize_);             
            fsQueue_.add(fs);               
        }
    }

