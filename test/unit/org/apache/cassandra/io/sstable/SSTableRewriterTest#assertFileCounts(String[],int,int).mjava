    private void assertFileCounts(String [] files, int expectedtmplinkCount, int expectedtmpCount)
    {
        int tmplinkcount = 0;
        int tmpcount = 0;
        for (String f : files)
        {
            if (f.contains("-tmplink-"))
                tmplinkcount++;
            if (f.contains("-tmp-"))
                tmpcount++;
        }
        assertEquals(expectedtmplinkCount, tmplinkcount);
        assertEquals(expectedtmpCount, tmpcount);
    }

