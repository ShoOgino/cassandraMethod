    /**
     * Format the test for printing..
     * @param test a test
     * @return the formatted testname
     */
    protected String formatTest(Test test) {
        if (test == null) {
            return "Null Test: ";
        } else {
            if (!tag.isEmpty())
                return "Testcase: " + test.toString() + "-" + tag + ":";
            return "Testcase: " + test.toString() + ":";
        }
    }

