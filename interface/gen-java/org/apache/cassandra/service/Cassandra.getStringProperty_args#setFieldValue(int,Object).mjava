    public void setFieldValue(int fieldID, Object value) {
      switch (fieldID) {
      case PROPERTYNAME:
        if (value == null) {
          unsetPropertyName();
        } else {
          setPropertyName((String)value);
        }
        break;

      default:
        throw new IllegalArgumentException("Field " + fieldID + " doesn't exist!");
      }
    }

