    private List<ByteBuffer> getRequestedBound(Bound b, List<ByteBuffer> variables) throws InvalidRequestException
    {
        assert isColumnRange();
        return buildBound(b,
                          new ArrayList<Name>(cfDef.clusteringColumns()),
                          columnRestrictions,
                          isReversed,
                          cfDef,
                          cfDef.getColumnNameBuilder(),
                          variables);
    }

