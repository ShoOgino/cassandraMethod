    public static String[] getTokens(String str, String delim)
    {
        StringTokenizer st = new StringTokenizer(str, delim);
        String[] values = new String[st.countTokens()];
        int i = 0;
        while ( st.hasMoreElements() )
	    {
		values[i++] = (String)st.nextElement();
	    }
        return values;
    }

