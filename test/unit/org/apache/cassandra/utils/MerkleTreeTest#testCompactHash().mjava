    @Test
    public void testCompactHash()
    {
        byte[] val = DUMMY;
        byte[] valXval = hashed(val, 1, 1);

        // (zero, four], (four,zero]
        mt.split(tok(4));

        // validate both ranges
        mt.get(tok(4)).hash(val);
        mt.get(tok(0)).hash(val);

        // compact (zero, eight]
        mt.compact(tok(4));
        assertHashEquals(valXval, mt.get(tok(0)).hash());
    }

